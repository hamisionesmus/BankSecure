{
  "version": 2,
  "builds": [
    {
      "src": "atm-app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "api/test.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/authenticate.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/accounts/[customerId].js",
      "use": "@vercel/node"
    },
    {
      "src": "api/transactions/[accountId].js",
      "use": "@vercel/node"
    },
    {
      "src": "api/balance/[accountId].js",
      "use": "@vercel/node"
    },
    {
      "src": "api/withdraw.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/deposit.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/transfer.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/atm-app/$1"
    }
  ]
}